    <lbm:familyRelation type="http://libmeta.ru/relation/family#related">
      <lbm:value rdf:resource="http://libmeta.ru/thesaurus/concept/fme_2006_OBLAST"/>
    </lbm:familyRelation>
    <lbm:familyRelation type="http://libmeta.ru/relation/family#related">
      <lbm:value rdf:resource="http://libmeta.ru/thesaurus/concept/fme_2006_OBLAST"/>
